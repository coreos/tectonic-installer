apiVersion: v1
kind: Config
clusters:
- name: ${cluster_name}
  cluster:
    server: ${server}
    certificate-authority-data: ${kube_ca_cert}
users:
- name: admin
  user:
    client-certificate-data: ${admin_cert}
    client-key-data: ${admin_key}
contexts:
- context:
    cluster: ${cluster_name}
    user: admin
