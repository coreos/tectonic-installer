apiVersion: v1
kind: Config
clusters:
- name: local
  cluster:
    server: ${server}
    certificate-authority-data: ${ca_cert}
users:
- name: kubelet
  user:
    client-certificate-data: ${kubelet_cert}
    client-key-data: ${kubelet_key}
contexts:
- name: local
  context:
    cluster: local
    user: kubelet
current-context: local
